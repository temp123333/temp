{
  "expo": {
    "name": "Ghumne",
    "slug": "ghumne",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Flag_of_Nepal.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#87cefa"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amritsapkota.exponative"
    },
    "android": {
      "package": "com.amritsapkota.exponative",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "11ced189-46fa-46c9-8208-caa1a1945f50"
      }
    },
    "owner": "amritsapkota"
  }
}
